public class  IfStatements : MonoBehaviour
{
    float swordStabs = 6.0f;
    float deathByStabs = 8.0f;
    float aliveByStabs = 3.0f;

    public float SwordandDeath { get; private set; }

    void Update ()
    {
        if (int.GetKeyDown(KeyCode.Space))
            StabTest();

        swordStabs -= SwordandDeath *.5f;


   public void StabTest();
    {
        if (swordStabs > deathByStabs)
        {
           Debug.Log("Whoops Bro, you're dead.");
        }
        else if (swordStabs < aliveByStabs)
        {
            Debug.Log("You just got Stabbed. Don't do that.");
        }
        else
        {
            Debug.Log("Still Healthy, I think.");
        }
    }
}
